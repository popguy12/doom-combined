::Check for wad file. If it's not there, jump out
if not exist "%~3\freedoom1.wad" (
    echo freedoom1.wad not found. Skipping... >&2
    EXIT /B %ERRORLEVEL%
)

cd "%~5"

::Copy our installer files
type "ANIMDEFS.freedoom1.prt" >> "%~2\ANIMDEFS.txt"
type "TEXTURES.freedoom1.prt" >> "%~2\TEXTURES.txt"
type "MAPINFO.freedoom1.prt" >> "%~2\MAPINFO.txt"

::Extract Files
::=======================
::call "%~1\display_section.cmd" Doom 1 >&2
echo Fetching data from freedoom1.wad... >&2
if not exist "%~3\freedoom1" mkdir "%~3\freedoom1"
cd "%~3\freedoom1"
call %4 "%~3\freedoom1.wad"
echo Copying data to %~2... >&2

